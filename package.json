{
  "name": "dolphinmade-cli",
  "version": "1.6.0",
  "description": "CLI tool for composable codebase edits - pricing, AI assistant, and page creation",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "dolphinmade": "./dist/cli.js",
    "dolphin": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format esm --dts --clean --shims",
    "dev": "tsup src/cli.ts src/index.ts --format esm --dts --watch --shims",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests yet\""
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "keywords": [
    "cli",
    "scaffolding",
    "code-generator",
    "pricing",
    "ai-assistant",
    "page-generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "prompts": "^2.4.2",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.9",
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.0.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
